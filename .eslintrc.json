{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "array-callback-return": 2,
        "arrow-body-style": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "curly": 2,
        "func-call-spacing": 2,
        "keyword-spacing": 2,
        "lines-around-comment": 2,
        "indent": 2,
        "linebreak-style": 2,
        "no-multiple-empty-lines": 2,
        "no-return-await": 2,
        "no-trailing-spaces": 2,
        "no-var": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "require-await": 2,
        "quotes": [
            "error",
            "single"
        ],
        "semi": 2,
        "space-unary-ops": 2,
        "spaced-comment": 2
    }
}
